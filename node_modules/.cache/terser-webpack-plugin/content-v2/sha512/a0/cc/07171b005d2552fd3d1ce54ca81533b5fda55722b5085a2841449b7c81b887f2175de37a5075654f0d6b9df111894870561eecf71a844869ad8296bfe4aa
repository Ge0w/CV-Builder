        <label>I work here currently</label>\n                    <input type='checkbox' checked={dateChecked} name='current'\n                    value={dateChecked} onChange={(e)=> setDateChecked(e.currentTarget.checked)}></input>\n                </div>\n                <div className='form-control'>\n                <label>To</label>\n                    <input type='date' name='to'\n                    value={dateTo} onChange={(e)=> setDateTo(e.target.value)}></input>\n                </div>\n                <div className='form-control'>\n                    <textarea type='textarea' name='description' placeholder='Description of Role' required value={description} onChange={(e)=> setDescription(e.target.value)}></textarea>\n                </div>\n                <input type='submit' value='Add' className='btn my-1'></input>\n            </form>\n        </div>\n    )\n}\n\nexport default PracticalExp\n","import { FaTimes } from 'react-icons/fa'\n\nconst Experience = ({ exp, onDelete }) => {\n    return (\n        <div>\n            <h3>{exp.company} <FaTimes style={{color:'red',cursor:'pointer'}} onClick={() => onDelete(exp.id)} /></h3>\n            <h3>{exp.role}</h3>\n            <h4>{exp.dateFrom} - {exp.dateChecked ? 'Present' : exp.dateTo}</h4>\n            <p>{exp.description}</p>\n        </div>\n    )\n}\n\nexport default Experience\n","const CVHeader = ({ infos }) => {\n    return (\n        <div>\n            <h1>{infos.firstname} {infos.lastname}</h1>\n            <h2>{infos.role}</h2>\n        </div>\n    )\n}\n\nexport default CVHeader\n","import { FaTimes } from 'react-icons/fa'\n\nconst EduInfo = ({ edu, onDelete }) => {\n    return (\n        <div>\n            <h3>{edu.school} <FaTimes style={{color:'red',cursor:'pointer'}} onClick={() => onDelete(edu.id)}/></h3>\n            <h3>{edu.level} in {edu.courseSubject}</h3>\n            <h4>{edu.dateFrom} to {edu.dateTo}</h4>\n        </div>\n    )\n}\n\nexport default EduInfo\n","const CVFooter = ({ infos }) => {\n    return (\n    <div className='references flex'>\n        <h2>Contact</h2>\n        <p>{infos.firstname} {infos.lastname} | {infos.email} | {infos.mobile} </p>\n        <p>References available on request</p>\n    </div>\n    )\n}\n\nexport default CVFooter\n","import Experience from \"./CV Components/Experience\";\nimport CVHeader from \"./CV Components/CVHeader\"\nimport EduInfo from \"./CV Components/EduInfo\"\nimport CVFooter from \"./CV Components/CVFooter\"\n\nconst PersonalCV = ({ infos, exps, onDelete, edus }) => {\n    return (\n        <div className='cv-container flex card'>\n            <div className='name-header flex'>\n                <CVHeader key={infos.id} infos={infos}/>\n            </div>\n            <div className='cv-body'>\n                <div className='grid'>\n                    <div className='experience my-1'>\n                        <h2>Experience</h2>\n                        {exps.map((exp) => (\n                        <Experience key={exp.id} exp={exp} onDelete={onDelete}/>\n                         ))}\n                    </div>\n                    <div className='personal-description'>\n                        <h2>Personal Brief</h2>\n                        <p>{infos.description}</p>\n                    </div>\n                    <div className='education my-1-2'>\n                    <h2>Education</h2>\n                    {edus.map((edu) => (\n                        <EduInfo key={edu.id} edu={edu} onDelete={onDelete}/>\n                         ))}\n                    </div>\n                </div>\n            </div>\n            <div className='name-header flex'>\n                    <CVFooter infos={infos}/>\n                </div> \n        </div>\n    )\n}\n\nexport default PersonalCV\n","import { useState } from 'react'\nimport Header from \"./components/Header\";\nimport General from \"./components/Inputs/General\";\nimport Educational from \"./components/Inputs/Educational\";\nimport PracticalExp from \"./components/Inputs/PracticalExp\";\nimport PersonalCV from \"./components/PersonalCV\";\n\nfunction App() {\n  // Set General Info\n  const [infos, setInfos] = useState({\n    id: 1,\n    firstname: 'Name',\n    lastname: '',\n    role: 'Role',\n    email: 'Email',\n    mobile: 'Mobile number',\n    description: 'Personal Description Here'\n});\n\n  // Set Experience\n  const [exps, setExps] = useState([]);\n\n  // Set Education\n  const [edus, setEdu] = useState([]);\n\n  // Change Info\n  const changeInfos = (info) => {\n    const id = Math.floor(Math.random() * 10000 + 1)\n    setInfos({id, ...info})\n  }\n\n\n  // Add Exp\n  const addExp = (exp) => {\n    const id = Math.floor(Math.random() * 10000 + 1)\n    const newExp = {id, ...exp}\n    setExps([...exps, newExp])\n  }\n\n  // Delete Exps\n  const deleteInfo = (id) => {\n    setExps(exps.filter((exp)=> exp.id !== id))\n    setEdu(edus.filter((edu)=> edu.id !== id))\n  }\n\n  // Add Education\n  const addEdu = (edu) => {\n    const id = Math.floor(Math.random() * 10000 + 1)\n    const newEdu = {id, ...edu}\n    setEdu([...edus, newEdu])\n  }\n\n  return (\n    <div className='container'>\n      <Header />\n      <div className='flex'>\n        <General onAdd={changeInfos}/>\n        <Educational onAdd={addEdu}/>\n        <PracticalExp onAdd={addExp}/>\n        <PersonalCV edus={edus} infos={infos} exps={exps} onDelete={deleteInfo}/>\n      </div>\n    </div>\n  );\n}\n\nexport default App;\n","import React from 'react';\nimport ReactDOM from 'react-dom';\nimport \"./components/Fontawesome\";\nimport './index.css';\nimport App from './App';\n\nReactDOM.render(\n  <React.StrictMode>\n    <App />\n  </React.StrictMode>,\n  document.getElementById('root')\n);"]}}